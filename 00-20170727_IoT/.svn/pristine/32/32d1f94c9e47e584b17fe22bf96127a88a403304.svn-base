  <select id="queryAllMaintainInfo" resultMap="BaseResultMap" parameterType="edu.hnu.csee.luo.model.Maintain" >
    select 
 	m.entityId ,e.entityType,e.detailAddress,m.maintainTime,m.status,m.recordTime,m.cause,e.area,u.userName,u.tel,m.maintainPeople,m.maintainMemTel,m.remark
	from maintain_info m 
	left join entity_info e
	on m.entityId = e.entityId 
	left join user_info u
	on e.userId=u.userId
	<where>  
	 	<if test="1==1">
	 		1=1
	 	</if>
        <if test="entityid!=null and entityid!= '' ">     
           and m.entityId = #{entityid}      
        </if>    
        <if test="entity!=null and entity.entityType!=null and entity.entityType!='' ">     
           and e.entityType = #{entity.entityType}      
        </if>
        <if test="condition!=null and condition.startTime!=null and condition.startTime!='' and condition.endTime!=null and condition.endTime!='' ">     
           and m.recordTime between #{condition.startTime} and #{condition.endTime}
           </if> 
    </where>  
   	limit #{paging.begin},#{paging.pageSize}
</select>

