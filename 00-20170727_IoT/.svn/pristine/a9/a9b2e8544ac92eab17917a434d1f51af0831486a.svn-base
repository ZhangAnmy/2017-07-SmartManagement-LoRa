													+ errorMsg + '</span></td>');
								}
							}

							if ($(this).is('#realname')) {
								var myReg = /^[\u4e00-\u9fa5]+$/;
								var myReg1 = /^[a-zA-Z]+$/;
								if (!(myReg.test($("input[id=realname]").val()))
